{
  "name": "shikisai",
  "version": "0.1.3",
  "description": "shikisai color-unit management tool",
  "license": "MIT",
  "keywords": [
    "color",
    "css",
    "scss"
  ],
  "private": false,
  "main": "./dist/index.js",
  "bin": {
    "shikisai": "./dist/index.js"
  },
  "scripts": {
    "start": "run-s clear watch",
    "watch": "run-p watch:*",
    "watch:asset": "cpx static/view/** dist/view -w",
    "watch:ts": "node --loader ts-node/esm scripts/esbuild.ts -w",
    "build": "run-p build:*",
    "build:asset": "cpx static/view/** dist/view",
    "build:ts": "node --loader ts-node/esm scripts/esbuild.ts",
    "lint": "run-s lint:*",
    "test": "jest",
    "lint:ts": "eslint",
    "update": "ncu -i",
    "fmt": "prettier . -w",
    "clear": "node --loader ts-node/esm scripts/clear.ts",
    "prebuild": "npm run clear",
    "prepublishOnly": "npm run build"
  },
  "author": "ken7253 <ken7253@gmail.com> (https://dairoku-studio.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/ken7253/shikisai"
  },
  "engines": {
    "node": "^14.16.0",
    "vscode": "^1.22.0"
  },
  "volta": {
    "node": "14.16.0"
  },
  "devDependencies": {
    "@types/color-convert": "^2.0.0",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.13",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "cpx": "^1.5.0",
    "esbuild": "^0.14.44",
    "eslint": "^8.13.0",
    "jest": "^27.5.1",
    "npm-check-updates": "^12.5.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "cac": "^6.7.12",
    "color-convert": "^2.0.1",
    "inquirer": "^8.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serve": "^13.0.2"
  }
}
